/* pages/fabu/fankui.wxss */
.fankui{
  background-color: #353535;
  color: white;
  text-align: center;
  line-height: 80rpx;
  width: 100%;
}
.kbanner{
  background-color: white;
  padding: 30rpx;
}
.kbantext{
  width: 97%;
  height: 280rpx;
  margin-bottom: 20rpx;
  border:1rpx solid #f2f2f2;
  border-radius: 10rpx;
  padding: 10rpx;
  background: #fff;
  margin: 0 auto 30rpx;
}
.kcamera{
  width: 79rpx;
  height: 79rpx;
  margin-right: 20rpx;
}
.kimgbox{
  display: flex;
  flex-wrap: wrap;
  padding:30rpx 0rpx;
  background-color: white;
}
.fankuiimg{
  position: relative;
  width: 140rpx;
  height: 200rpx;
  margin: 15rpx 30rpx 15rpx 15rpx;
}
.upfile{
  width: 100%;
  height: 100%;
  background-color: pink;
}
.shanchu{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  z-index: 3;
  right: -25rpx;
  top: -15rpx;
}

.fsave{
  background-color: #F34B4B;
  color: white;
  width: 90%;
  margin-top: 50rpx;
}
.fdelete{
  background-color: #F8F8F8;
  margin-top: 40rpx;
  width: 90%;
  border: 2rpx solid #DCDCDC;
  margin-left: auto;
  margin-right: auto;
  text-align: center;
  line-height: 85rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
}
