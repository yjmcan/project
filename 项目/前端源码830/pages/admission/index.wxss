/* pages/admission/index.wxss */
.head{
  /* padding: 20rpx 40rpx; */
  width: 95%;
  margin: 20rpx auto 0;
  height: 400rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.member{
  width: 100%;
  margin: 0 auto 0;
  height: 100%;
}
.member image{
  width: 100%;
  height: 100%;
}
.list_0{
  width: 25%;
}
.list_1{
  width: 75%;
}
.choose{
  width: 25%;
  height: 180rpx;
  margin-top: 20rpx;
}
.choose>image{
  width: 95%;
  height: 100%;
}
.choose_img{
}
.list_1>view:nth-child(1){
  width: 85%;
}
.list_1>view:nth-child(2){
  width: 15%;
  box-sizing: border-box;
  text-align: right;
  border-left: 2rpx solid #e5e5e5;
}
.list_1>view image{
  width: 40rpx;
  height: 40rpx;
  background: red;
}
.route>view view{
  height: 60rpx;
}
.route>view view:nth-child(1){
  width: 31%;
}
.route>view view:nth-child(2){
  width: 8%;
}
.route>view view:nth-child(3){
  width: 31%;
}
.route>view view:nth-child(4){
  width: 30%;
}
.route>view view text:nth-child(1){
  padding: 6rpx 20rpx;
  border: 2rpx solid red;
  border-radius: 10rpx;
  color: red;
}
.route>view view text:nth-child(2){
  padding: 6rpx 20rpx;
  margin-left: 10rpx;
  border-radius: 10rpx;
}
button{
  border-radius: 0;
}