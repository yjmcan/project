/*kpgg*/
.zhezhao {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 998;
}
.kpgg {
   width:600rpx;
  left:50%;
  top:100rpx;
  position:fixed;
  transform:translate(-50%, 0);
  background:#fff;
  border-radius:30rpx;
  z-index:999;
}
.kpgg-swiper{
  display: block;
  height: 900rpx;
  border-radius: 30rpx;
}
.kpgg-swiper image{
  border-radius: 30rpx;
}
.close-kpgg{
  right: -40rpx;
  top: -40rpx;
  z-index:1001;
  position: absolute;
  width:80rpx;
  height: 80rpx;
}
.selected{
  color: #000;
  font-weight: bold;
}
.selected .xsjt-img{
  transform: rotate(180deg)
}
.default{
  color: #666;
}
