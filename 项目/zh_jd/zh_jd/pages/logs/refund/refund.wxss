/**index.wxss**/ 
page{
  background: #f5f5f5;
}
page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    padding-top: 60px;
}
.weui-navbar__slider{
  width: 120rpx;
}
.weui-tab__content{
  padding-top: 0;
}
.weui-cells{
  margin-top: 20rpx;
}
.weui-navbar{
  background-color: white;
  border-bottom: none;
}
.cell-one{
  width: 100%;
  display: flex;
}
.cell-two,
.cell-three{
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
}
.cell-two{
  font-size: 28rpx;
  color: #A9A9A9;
}
.cell-three{
  font-size: 24rpx;
  color: #A9A9A9;
}
.weui-navbar__item.weui-bar__item_on{
  color: #EB5E18;
}
.weui-navbar__slider{
  background-color: #EB5E18;
}
.mylist1{
  padding: 30rpx;
  margin-top: 30rpx;
  border-top:1rpx solid #e0e0e0;
  border-bottom:1rpx solid #e0e0e0;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
}
.mylist_banner2{
  color: #000;
}
.mylist_title{
  font-size: 28rpx;
  display: flex;
   align-items: center; 
  justify-content: space-between;
}
.mylist_title1{
  color: #EB5E18;
}
.mylist_title2{
  font-size: 26rpx;
  color: #999;
}
.mylist_title4{
  margin-left: 100rpx;
}
.mylist_banner1{
  display: flex;
  align-items: center;
  padding: 15rpx 30rpx;
  justify-content: space-between;
  background-color: white;
  border-bottom:1rpx solid #e0e0e0;
}
.mylist_banner3{
  display: flex;
}
.mylist_banner4{
  width: 150rpx;
  height: 70rpx;
  border-radius: 10rpx;
  display: flex;
  color: #000;
  align-items: center;
   justify-content: center; 
  border: 1rpx solid #ccc;
}
.mylist_banner5{
  width: 150rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #EB5E18;
  border-radius: 10rpx;
  border: 1rpx solid #EB5E18;
  margin-left: 15rpx;
}
.mylist_banner5 button{
    background: #fff;
    width: 100%;
    height: 100%;
    color: #EB5E18;
    padding: 0;
    line-height: 70rpx;
    font-size: 26rpx;
}
.mylist_banner5 button::after{
  border: none;
}
.mylist_footer{
  font-size: 26rpx;
  color: #999;
  text-align: center;
  margin-top: 30rpx;
}
.mylist_back1{
  margin-top:40%;
  margin-left: 30%;
}
.aaa{
  text-align: center;
  font-size: 28rpx;
  color: #999;
}
.border{
  border-top: 1rpx solid #E9E9E9;
}
.mylist_back{
  display: flex;
  flex-direction: column;
   /* padding-top: 40%;  */
  color: #999;
  font-size: 28rpx;
}
.mylist_back2{
  width: 324rpx;
  height: 302rpx;
}
