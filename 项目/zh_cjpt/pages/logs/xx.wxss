/* pages/logs/xx.wxss */
.list>view text{
  display: block;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #f66925;
}
.list>view:nth-child(1){
  width: 5%;
}
.list>view:nth-child(2){
  width: 15%;
}
.list>view:nth-child(3){
  width: 80%;
}
.list>view:nth-child(4){
  padding: 10rpx 0 0 0;
}
.list{
  padding: 30rpx 20rpx;
  background: #fff;
}