  width: 100%;
  height: 240rpx;
}
.order_num{
  width: 95%;
  height: 100%;
  position: fixed;
  top: 0;
}
.order_head{
  display: flex;
}
.order_head>view{
  flex: 2;
}
.order_head>view:nth-child(1){
  border-right: 2rpx solid #fff;
}
