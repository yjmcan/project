  display: flex;
  height: 190rpx;
  border-radius: 8rpx;
  overflow: hidden;
  padding: 40rpx 20rpx;
}
.list_0>view{
  flex: 2;
}
.list_0>view:nth-child(1){
  border-right: 2rpx solid #e5e5e5;
}
.list_0>view image{
  width: 80%;
