<view class=' tabbar'>
  <view bindtap='tabbar' wx:for="{{menu}}" wx:key="*this" bindtap='route_page' data-url='{{item.url}}'>
    <image mode='aspectFill' src='{{item.active?item.sele_img:item.img}}'></image>
    <text style="color:{{item.active?color:item.color}}">{{item.name}}</text>
  </view>
</view>