.list {
  padding: 30rpx 20rpx;
}

.list_head {
  height: 25rpx;
  line-height: 25rpx;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #e5e5e5;
}

.list_head text {
  display: inline-block;
}

.list_head text:nth-child(1) {
  background: #f66926;
  width: 6rpx;
  height: 100%;
}

.list_head text:nth-child(2) {
  height: 100%;
  margin-left: 10rpx;
}

.img_list{
  padding: 20rpx 0;
}
.img_list>image{
  width: 100%;
}
.examine {
  background: #fff;
  margin-top: 30rpx;
}

.user_img, .user_info, .examine_status {
  display: inline-block;
  vertical-align: middle;
  height: 150rpx;
  margin-top: 30rpx;
}

.user_img {
  width: 20%;
}

.user_img image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}

.user_info {
  width: 55%;
  margin-left: 5%;
}

.examine_status {
  width: 20%;
}

.user_info>view {
  width: 100%;
  height: 50%;
}

.user_info>view:nth-child(1) {
  line-height: 95rpx;
}

.user_info>view:nth-child(2) {
  line-height: 55rpx;
  font-size: 22rpx;
  color: #999;
}

.user_info>view text {
  display: inline-block;
}

.user_info>view text:nth-child(1) {
  font-size: 32rpx;
  max-width: 200rpx;
}

.user_info>view text:nth-child(2) {
  font-size: 26rpx;
  margin-left: 20rpx;
  color: #f66926;
  max-width: 150rpx;
}

.examine_status text {
  padding: 10rpx 20rpx;
  background: orange;
  font-size: 24rpx;
  color: #fff;
  border-radius: 3rpx;
  top: 40rpx;
}

.registration {
  padding: 40rpx;
  text-align: center;
  background: #fff;
}

.registration_info {
  background: #fff;
}

.registration_info>view {
  padding: 25rpx 20rpx;
  border-bottom: 2rpx solid #e5e5e5;
}

.registration_info>view:last-child {
  border: none;
}
.sign_up_status{
  margin: 20rpx auto 0rpx;
  width: 95%;
  background: #fff;
  height: 150rpx;
  border-radius: 10rpx;
}
.sign_up_status>view:nth-child(1){
  width: 95%;
  height: 50rpx;
  margin-left: 3%;
  padding: 20rpx 0 0 0;
}
.sign_up_status>view image{
  display: inline-block;
  vertical-align: middle;
  width: 40rpx;
  height: 40rpx;
}
.sign_up_status>view view{
  display: inline-block;
  vertical-align: middle;
  color: #000;
}
.sign_up_status>view:nth-child(2){
  bottom: 0;
  width: 97%;
  padding-bottom: 20rpx;
  right: 3%;
  text-align: right;
}
.check_examine{
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: 0;
  background: #fff;
}
.check_examine>view{
  float: left;
  width: 50%;
  height: 100%;
}
button::after{
  border: none;
}
.check_examine>view button{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.button_refuse{
  color: #f66926;
  background: #f9f9f9;
  border: 2rpx solid #e5e5e5;
}
.button_agree{
  color: #fff;
  background: #f66926;
}