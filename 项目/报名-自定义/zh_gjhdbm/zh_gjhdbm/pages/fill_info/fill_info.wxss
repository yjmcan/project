.etitle {
  border-bottom: 1rpx solid #eee;
  display: flex;
  align-items: center;
  padding: 20rpx;
  background: #fff;
  position: relative;
}

.etitle>view:nth-child(3) {
  position: absolute;
  color: blue;
  right: 5%;
  font-size: 24rpx;
}

.exian {
  width: 8rpx;
  height: 30rpx;
  margin-right: 15rpx;
  background-color: #f66926;
}

.ticket, .total {
  padding: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
}

.ticket {
  border-bottom: 2rpx solid #e5e5e5;
}

.ticket>view {
  float: left;
}

.ticket>view:nth-child(1) {
  width: 60%;
}

.ticket>view:nth-child(2) {
  width: 20%;
}

.ticket>view:nth-child(3) {
  width: 20%;
  color: #f66926;
}

.total>view {
  float: right;
}

.total>view:nth-child(1) {
  font-size: 34rpx;
  color: #f66926;
  font-family: '微软雅黑';
}

.total>view:nth-child(2) {
  font-size: 24rpx;
  color: #666;
}

.sign {
  margin-top: 20rpx;
  /* padding-bottom: 120rpx; */
  background: #fff;
}

.sign_up {
  padding: 0 40rpx;
  background: #fff;
}

.sign_up>view:nth-child(1) {
  line-height: 80rpx;
  height: 80rpx;
}

.sign_up>view:nth-child(2) {
  line-height: 70rpx;
  height: 70rpx;
  border: 2rpx solid #e5e5e5;
}

.sign_up>view text {
  color: #f66926;
}

.sign_up>view input {
  width: 90%;
  height: 100%;
  margin-left: 2%;
  font-size: 26rpx;
}
button{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  position: fixed;
  bottom: 20rpx;
  background: #f66926;
  color: #fff;
  left: 5%;
  z-index: 5000000;
}
button::after{
  border: none;
}
.upload_img{
  padding: 20rpx 14rpx 20rpx 20rpx;
}
.upload_img_show{
}
.sele_upload_img{
  /* background: orchid; */
  border: 6rpx solid #e5e5e5;
  box-sizing: border-box;
}
.upload_img>view{
  width: 25%;
  margin-top: 5rpx;
  height: 170rpx;
  display: inline-block;
  vertical-align: middle;
}
.upload_img_show image{
  width: 97%;
  height: 100%;
}
.sele_upload_img image{
  width: 60%;
}
.notice{
  padding: 20rpx;
}
.notice .icon{
  top:23rpx;
  left: 20rpx;
  box-sizing: border-box;
  border-radius: 50%;
  border: 2rpx solid #e5e5e5;
}
.mar_40{
  margin-left: 40rpx;
}
.color_9{
  color: #999;
}