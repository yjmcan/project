/*modal*/
.modal {
  width: 76%;
  left: 12%;
  top: 20%;
  position: fixed;
  background: #fff;
  border-radius: 30rpx;
  z-index: 1001;
  -webkit-transform: scale(0.0);
  -moz-transform: scale(0.0);
  -ms-transform: scale(0.0);
  transform: scale(0.0);
  opacity: 0;
  -webkit-transition: all 0.6s;
  -moz-transition: all 0.6s;
  transition: all 0.6s;
}
.modal.active {
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
  opacity: 1;
}
.mpmodal {
  width: 76%;
