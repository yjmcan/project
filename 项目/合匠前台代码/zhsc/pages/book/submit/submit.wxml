<form bindsubmit="submit" reportSubmit="true"><view class="population"><view class="goods flex-row"><view class="goods-img flex-grow-0"><image src="{{goods.cover_pic}}"></image></view><view class="flex-grow-1 flex-col goods-info"><view class="flex-grow-1 goods-name">\n                    {{goods.name}}\n</view><view class="flex-gorw-0 flex-row money flex-y-bottom"><view class="original_price flex-grow-0">\n                        ¥{{goods.original_price}}\n</view><view class="price flex-grow-0">\n                        ¥{{goods.price}}\n</view></view></view></view><view class="form"><view class="form-image"><image src="/images/yy-form-title.png"></image></view><view class="group-form"><block wx:for="{{form_list}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{item.type=='text'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                    {{item.name}}：\n</view><view class="flex-grow-1"><input bindinput="inputChenge" data-id="{{index}}" placeholder="{{item.tip}}" value="{{item.default}}"></input></view></view></block><block wx:if="{{item.type=='radio'}}" ><view class="group-radio flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                    {{item.name}}：\n</view><view class="flex-grow-1" style="padding-top:24rpx;"><block wx:for="{{item.default}}" wx:for-item="de" wx:for-index="idx"><label bindtap="radioChange" class="checkbox group-radio-item {{de.selected?'active':''}} flex-y-cente" data-id="{{idx}}" data-pid="{{index}}"><checkbox checked="{{item.selected}}" data-id="0" style="display:none" value="{{de.name}}"></checkbox>{{de.name}} \n</label></block></view></view></block><block wx:if="{{item.type=='checkbox'}}" ><view class="group-radio flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                    {{item.name}}：\n</view><view class="flex-grow-1" style="padding-top:24rpx;"><block wx:for="{{item.default}}" wx:for-item="de" wx:for-index="idx"><label bindtap="checkboxChange" class="checkbox group-radio-item {{de.selected?'active':''}} flex-y-cente" data-id="{{idx}}" data-pid="{{index}}"><checkbox checked="{{item.selected}}" data-id="0" style="display:none" value="{{de.name}}"></checkbox>{{de.name}} \n</label></block></view></view></block><block wx:if="{{item.type=='time'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                    {{item.name}}：\n</view><view class="flex-grow-1 flex-row picker-data flex-y-center"><picker bindchange="inputChenge" class="flex-grow-0" data-id="{{index}}" mode="time" style="padding-right:20rpx;" value="{{item.default}}"><view class="picker flex-y-center">\n                                            {{item.default}}\n</view></picker><view class="flex-grow-0 picker-time flex-y-center"><image src="/images/img-share-right.png"></image></view></view></view></block><block wx:if="{{item.type=='date'}}" ><view class="username flex-row input-shell flex-y-center"><view class="flex-grow-0">\n                                    {{item.name}}：\n</view><view class="flex-grow-1 flex-row picker-data flex-y-center"><picker bindchange="inputChenge" class="flex-grow-0" data-id="{{index}}" mode="date" style="padding-right:20rpx;" value="{{item.default}}"><view class="picker flex-y-center">\n                                            {{item.default}}\n</view></picker><view class="flex-grow-0 picker-time flex-y-center"><image src="/images/img-share-right.png"></image></view></view></view></block><block wx:if="{{item.type=='textarea'}}" ><view class="username flex-row input-shell flex-y-top" style="padding:24rpx 0;"><view class="flex-grow-0 flex-y-top">\n                                    {{item.name}}：\n</view><view class="flex-grow-1 flex-row flex-y-center"><textarea bindinput="inputChenge" data-id="{{index}}" placeholder="{{item.tip}}"></textarea></view></view></block></block></view></view></view><button class="submit-btn flex-row flex-y-center flex-x-center" formType="submit">\n        提交预约\n</button></form>
