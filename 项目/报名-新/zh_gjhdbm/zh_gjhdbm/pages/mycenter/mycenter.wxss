/* pages/mycenter/mycenter.wxss */
.index{
  width: 100%;
  overflow-x: hidden;
}
.cenbanner{
  background-color: white;
  margin-top: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
}
.cenright{
  width: 20rpx;
  height: 30rpx;
}
.cenuserimg{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.cenbox{
  display: flex;
}
.cenusername{
  font-weight: bold;
  font-size: 28rpx;
  margin-top: 15rpx;
}
.cenbutton{
  background-color: #F66926;
  color: white;
  font-weight: bold;
  width: 90%;
  position: absolute;
  bottom: 40rpx;
  left: 5%;
  font-size: 34rpx;
}









