/* pages/kaci/kaci.wxss */
.kaci{
  width: 100%;
  height: 160rpx;
  background: #fff;
  margin-top: 20rpx;
}
.kaci>view{
  display: inline-block;
  vertical-align: middle;
  text-align: left;
  font-size: 28rpx;
  line-height: 160rpx;
}
.kaci>view:nth-child(1){
  width: 30%;
  height: 100%;
}
.kaci>view image{
  width: 160rpx;
  height: 120rpx;
}
.kaci>view:nth-child(2){
  width: 40%;
  height: 100%;
}
.kaci>view:nth-child(3){
  width: 30%;
  height: 100%;
}
.red{
  color: red;
}
