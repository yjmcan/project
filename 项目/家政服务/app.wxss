page{
  background: #f9f9f9;
}
.wid_hei{
  width: 100%;
  height: 100%;
}
.img{
  width: 100%;
  height: 100%;
}
.rel{
  position: relative;
}
.ab{
  position: absolute;
}
.fixed{
  position: fixed;
}
.ud{
  top: 0;
  bottom: 0;
  margin: auto;
}
.lr{
  left: 0;
  right: 0;
  margin: auto;
}
.udlr{
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.align{
  text-align: center;
}
.inline{
  display: inline-block;
  vertical-align: middle;
}

.gobyndsingle {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.gobyndmulit {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.copyright{
  width: 100%;
  height: 50rpx;
  text-align: center;
  vertical-align: middle;
  line-height: 50rpx;
  margin-top: 40rpx;
  margin-bottom: 30rpx;
  color: #666;
  font-size: 24rpx;
}
.copyright>image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  border-radius: 50%;
}